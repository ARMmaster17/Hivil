#HIVE:1.0
#NAME:sampleprogram
#START:main

// Entry point for the program
JOB main
  HIVEINT result
  INT computeid
  RUN compute computeid
  OUTPUT Here_is_the_result_of_operation_compute
  WAIT compute(computeid)
  OUTPUT result
  OUTPUT Press_any_key_to_quit
  PAUSE
ENDJOB

JOB compute
  ADD 2 2 result
ENDJOB
